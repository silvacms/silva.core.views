<html tal:omit-tag=""
      lang="en"
      xml:lang="en"
      xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <body>

    <form method="post" action="#" enctype="multipart/form-data"
          tal:attributes="action request/URL">

      <table class="public-form">
        <thead>
          <tr tal:condition="view/label">
            <th class="form-title" colspan="2"
                tal:content="view/label">
            </th>
          </tr>
        </thead>
        <tbody>
          <tr class="status-message"
              tal:condition="view/status">
            <th colspan="2" tal:content="view/status" />
          </tr>
          <metal:widgets metal:use-macro="view/form_macros/widgets" />
          <tr class="required-description">
            <th colspan="2" i18n:translate="">
              * required
            </th>
          </tr>
          <metal:widgets metal:use-macro="view/form_macros/actions" />
        </tbody>
      </table>
    </form>

  </body>

</html>
